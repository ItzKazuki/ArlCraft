<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html
</IfModule>

RewriteEngine On
RewriteCond %{HTTP_HOST} ^arlcraft.xyz [NC]
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST]%{REQUEST_URI} [R=301,L]
Options -Indexes
